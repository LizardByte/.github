{
  "problemMatcher": [
    {
      "owner": "shellcheck-gcc",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+):\\s+(?:fatal\\s+)?(error|note|warning):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 5
        }
      ]
    }
  ]
}

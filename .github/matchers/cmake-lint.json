{
  "problemMatcher": [
    {
      "owner": "cmake-lint",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(.+?):(\\d+),(\\d+): \\[([A-Z]\\d{4})\\] (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    }
  ]
}
